{
  "name": "@xxs3315/mbl-data-bindings",
  "version": "1.0.0",
  "description": "Data bindings for MixBoxLayout - Text和Table的数据绑定",
  "keywords": [
    "mbl",
    "mix-box-layout",
    "data",
    "binding",
    "数据",
    "数据绑定"
  ],
  "license": "MIT",
  "author": "xxs3315",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-esm",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm && pnpm run build:fix-output",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:fix-output": "node scripts/fix-output.js",
    "clean": "rimraf dist dist-esm",
    "dev": "tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@mantine/core": "^8.2.5",
    "@mantine/hooks": "^8.2.5",
    "@mantine/notifications": "^8.2.5",
    "ace-builds": "^1.43.3",
    "json-schema-generator": "^2.0.6",
    "lucide-react": "^0.541.0",
    "mac-scrollbar": "^0.13.8",
    "react-ace": "^14.0.1"
  },
  "devDependencies": {
    "@types/json-schema-generator": "^2.0.3",
    "@types/react": "~19.1.0",
    "@types/react-dom": "~19.1.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "~19.1.0",
    "react-dom": "~19.1.0"
  }
}
