{
  "name": "@xxs3315/mbl-providers",
  "version": "1.0.0",
  "description": "React Context Providers for MBL project - MBL项目的React Context Providers",
  "keywords": [
    "mbl",
    "providers",
    "react",
    "context",
    "theme",
    "i18n",
    "dpi"
  ],
  "license": "MIT",
  "author": "xxs3315",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist-esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-esm",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm && pnpm run build:fix-output",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:fix-output": "node scripts/fix-output.js",
    "clean": "rimraf dist dist-esm",
    "dev": "tsc -p tsconfig.build.json --watch",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@xxs3315/mbl-locales": "workspace:*",
    "@xxs3315/mbl-themes": "workspace:*",
    "react-i18next": "^15.1.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "rimraf": "~6.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "~19.1.0"
  }
}
